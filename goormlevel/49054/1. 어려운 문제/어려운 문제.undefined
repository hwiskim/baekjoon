# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean


def fact(num):
	dp = [0]*(num+1)
	dp[0] = 1
	for i in range(1, num+1):
		dp[i] = dp[i-1] * i
	return dp[num]

def make_result(num_2):
	real = 0
	while num_2:
		temp = num_2 % 10
		real = real + temp
		num_2 = num_2 // 10
	return real
	
N = int(input())
result = fact(N)

while result // 10 != 0:
	result = make_result(result)

print(result)